    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        Loca costoUber = new Loca();

        Loca ve = new Loca();
        double tarifaUberKilometro = 950.50;
        double tarifaTaxiKilometro = 881.12;
        double costoUSan, costoUS, costoUP, costoUF, costoSanU, costoSanS, costoSanF, costoSanP, costoFSan, costoFS, costoFU, costoFP, costoPU, costoPS, costoPSan, costoPF, costoSU, costoSP, costoSSan, costoSF;
        costoUSan = (int) (15.1 * tarifaUberKilometro);
        costoUS = (int) (30.8 * tarifaUberKilometro);
        costoUP = (int) (10.4 * tarifaUberKilometro);
        costoUF = (int) (20.9 * tarifaUberKilometro);
        costoSanU = (int) (13.4 * tarifaUberKilometro);
        costoSanS = (int) (7.3 * tarifaUberKilometro);
        costoSanF = (int) (17.4 * tarifaUberKilometro);
        costoSanP = (int) (11.6 * tarifaUberKilometro);
        costoFSan = (int) (4.8 * tarifaUberKilometro);
        costoFS = (int) (11.5 * tarifaUberKilometro);
        costoFU = (int) (10.4 * tarifaUberKilometro);
        costoFP = (int) (6.9 * tarifaUberKilometro);
        costoPS = (int) (13.0 * tarifaUberKilometro);
        costoPF = (int) (12.7 * tarifaUberKilometro);
        costoPSan = (int) (8.9 * tarifaUberKilometro);
        costoPU = (int) (11.7 * tarifaUberKilometro);
        costoSSan = (int) (20.3 * tarifaUberKilometro);
        costoSU = (int) (13.2 * tarifaUberKilometro);
        costoSF = (int) (15.7 * tarifaUberKilometro);
        costoSP = (int) (7.1 * tarifaUberKilometro);

        JOptionPane.showMessageDialog(FrmMenu.this, "                               UBER                \n\n\n                               USAQUEN\n" + "El costo de Usaquen a San Cristobal es = " + costoUSan + "\n El costo de Usaquen a Suba es = " + costoUS + "\n El costo de Usaquen a Fontibon es = " + costoUF + "\n El costo de Usaquen a Puente Aranda es = " + costoUP);
        JOptionPane.showMessageDialog(FrmMenu.this, "                               UBER                \n\n\n                          SAN CRISTOBAL\n" + "El costo de San Cristobal a San Usaquen es = " + costoSanU + "\n El costo de San Cristobal a Suba es = " + costoSanS + "\n El costo de San Cristobal a Fontibon es = " + costoSanF + "\n El costo de San Cristobal a Puente Aranda es = " + costoSanP);
        JOptionPane.showMessageDialog(FrmMenu.this, "                               UBER                \n\n\n                               FONTIBON\n" + "El costo de Fontibon a San Cristobal es = " + costoFSan + "\n El costo de Fontibon a Suba es = " + costoFS + "\n El costo de Fontibon a Usaquen es = " + costoFU + "\n El costo de Fontibon a Puente Aranda es = " + costoFP);
        JOptionPane.showMessageDialog(FrmMenu.this, "                               UBER                \n\n\n                               SUBA\n" + "El costo de Suba a San Cristobal es = " + costoSSan + "\n El costo de Suba a Fontibon es = " + costoSF + "\n El costo de Suba a Usaquen  es = " + costoSU + "\n El costo de Suba a Puente Aranda es = " + costoSP);
        JOptionPane.showMessageDialog(FrmMenu.this, "                               UBER                \n\n\n                       PUENTE ARANDA\n" + "El costo de Puente Aranda a San Cristobal es = " + costoPSan + "\n El costo de Puente Aranda a Suba es = " + costoPS + "\n El costo de Puente Aranda a Fontibon es = " + costoPF + "\n El costo de Puente Aranda a Usaquen es = " + costoPU);
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        double tarifaTaxiKilometro = 881.12;
        double costoUSan, costoUS, costoUP, costoUF, costoSanU, costoSanS, costoSanF, costoSanP, costoFSan, costoFS, costoFU, costoFP, costoPU, costoPS, costoPSan, costoPF, costoSU, costoSP, costoSSan, costoSF;
        costoUSan = (int) (15.1 * tarifaTaxiKilometro);
        costoUS = (int) (30.8 * tarifaTaxiKilometro);
        costoUP = (int) (10.4 * tarifaTaxiKilometro);
        costoUF = (int) (20.9 * tarifaTaxiKilometro);
        costoSanU = (int) (13.4 * tarifaTaxiKilometro);
        costoSanS = (int) (7.3 * tarifaTaxiKilometro);
        costoSanF = (int) (17.4 * tarifaTaxiKilometro);
        costoSanP = (int) (11.6 * tarifaTaxiKilometro);
        costoFSan = (int) (4.8 * tarifaTaxiKilometro);
        costoFS = (int) (11.5 * tarifaTaxiKilometro);
        costoFU = (int) (10.4 * tarifaTaxiKilometro);
        costoFP = (int) (6.9 * tarifaTaxiKilometro);
        costoPS = (int) (13.0 * tarifaTaxiKilometro);
        costoPF = (int) (12.7 * tarifaTaxiKilometro);
        costoPSan = (int) (8.9 * tarifaTaxiKilometro);
        costoPU = (int) (11.7 * tarifaTaxiKilometro);
        costoSSan = (int) (20.3 * tarifaTaxiKilometro);
        costoSU = (int) (13.2 * tarifaTaxiKilometro);
        costoSF = (int) (15.7 * tarifaTaxiKilometro);
        costoSP = (int) (7.1 * tarifaTaxiKilometro);

        JOptionPane.showMessageDialog(FrmMenu.this, "                               TAXI                \n\n\n                               USAQUEN\n" + "El costo de Usaquen a San Cristobal es = " + costoUSan + "\n El costo de Usaquen a Suba es = " + costoUS + "\n El costo de Usaquen a Fontibon es = " + costoUF + "\n El costo de Usaquen a Puente Aranda es = " + costoUP);
