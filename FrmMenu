    private void butsubaActionPerformed(java.awt.event.ActionEvent evt) {                                        

        Loca lp = new Loca();
        lp.dLsLf = 15.7;
        lp.dLsLp = 7.1;
        lp.dLsLsan = 20.3;
        lp.dLsLu = 13.2;
        JOptionPane.showMessageDialog(FrmMenu.this, "La distacia de Suba a  Fontibon  es = " + lp.dLsLf + "\n" + "La distacia de Suba a  Puente Aranda es = " + lp.dLsLp + "\n" + "La distacia de Suba a San Cristobal es = " + lp.dLsLsan + "\n" + "La distacia de Suba a  Usaquen es = " + lp.dLsLu);
        lp.setVisible(true);
        // TODO add your handling code here:
    }                                       

