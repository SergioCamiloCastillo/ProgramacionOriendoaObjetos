    private void butUzaqActionPerformed(java.awt.event.ActionEvent evt) {                                        

        Loca ve = new Loca();
        ve.dluAlp = 10.4;
        ve.dLuLs = 30.8;
        ve.dLuLf = 20.9;
        ve.dLuLSan = 15.1;

        JOptionPane.showMessageDialog(FrmMenu.this, "La distacia de Usaquen a  Puente Aranda es = " + ve.dluAlp + "\n" + "La distacia de Usaquen a  Suba es = " + ve.dLuLs + "\n" + "La distacia de Usaquen a  Fontibon = " + ve.dLuLf + "\n" + "La distacia de Usaquen a  San Cristobal es = " + ve.dLuLSan);

        ve.setVisible(true);
    }                                       

